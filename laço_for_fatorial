#include<stdio.h>

int main ()
{
	int i=0;
	int nf=6,pf=5,subf=0;
	int n=1,p=1, sub=1;
	int result=0;
	

	for(i=2; i<=nf; i++){
	
		printf("%d\n",i);
			n = n *i;
}
	printf(" fatorial de n é %d\n",n);
	for(i=2; i<=pf; i++){
	
		printf("%d\n",i);
			p = p *i;
}
	printf(" fatorial de p é %d\n",p);

subf = nf-pf;
for(i=2; i<=subf; i++){
	
		printf("%d\n",i);
			sub = sub *i;
}
	printf(" fatorial de sub  é %d\n",sub);	
	result = n/p*sub;
	
	printf(" arranjo de n,p e : %d",result);
}
