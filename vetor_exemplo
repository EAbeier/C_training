#include<stdio.h>
#define TAM_MAX 5

int main()
{
	int i=0, cont, soma, num;
	int vetor[TAM_MAX];
double maior;
	double menor;
	float media;

	for(i= 0; i<TAM_MAX; i++){ 
		printf("Digite um numero %d: ",i+1);
		scanf("%d",&vetor[i]);
		cont++;
		soma = soma+vetor[i];

	
	}
	maior = vetor[0];

	for(i=0; i<TAM_MAX; i++){
		printf("numero %d\n", vetor[i]);
		if (vetor[i]>maior){
			maior = vetor[i];
}
}
	menor = vetor[1];
	for(i=0; i<TAM_MAX; i++){	
		if(vetor[i]<menor){
			menor=vetor[i];
		}
		
	}
	media = soma/cont;
	printf("maior dos vetores %lf \n",maior);
	printf("menor dos vetores %lf \n",menor);
	printf("media dos vetores %2.0f \n",media);
}
